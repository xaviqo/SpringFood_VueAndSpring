                           </v-row>\n                            </template>\n                            <div class=\"fill-height bottom-gradient\"></div>\n                        </v-img>\n                    </v-avatar>\n                    <p class=\"white--text subheading mt-6 text-center overline\">Email</p>\n                </v-flex>\n            </v-layout>\n            <v-list flat two-line>\n                <v-list-item v-for=\"l in links\" :key=\"l.text\" dark :to=\"l.route\" color=\"white\" active-class=\"menu-border\">\n                    <v-list-item-action>\n                        <v-icon>{{l.icon}}</v-icon>\n                    </v-list-item-action>\n                    <v-list-item-content>\n                        <v-list-item-title>{{l.text}}</v-list-item-title>\n                    </v-list-item-content>\n                </v-list-item>\n            </v-list>\n        </v-navigation-drawer>\n    </nav>\n</template>\n<style>\n.v-toolbar__title{\n  width: 250px !important;\n}\n.menu-border {\n    border-left: 5px white solid;\n}\n</style>\n<script>\nimport App from '@/App.vue';\nexport default {\n    data: () => ({\n        drawer: true,\n        links: [\n            {\n                icon: \"mdi-home\",\n                text: \"Home\",\n                route: \"/\"\n            },\n            {\n                icon: \"mdi-clipboard-text\",\n                text: \"Dashboard\",\n                route: \"/admin\"\n            },\n            {\n                icon: \"mdi-account-group\",\n                text: \"Team\",\n                route: \"/team\"\n            },\n            {\n                icon: \"mdi-food\",\n                text: \"Products\",\n                route: \"/products\"\n            },\n            {\n                icon: \"mdi-test-tube\",\n                text: \"Test\",\n                route: \"/generator\"\n            }\n        ]\n    }),\n    methods: {\n        currentTime() {\n      let date = new Date();\n      let hh = date.getHours();\n      let mm = date.getMinutes();\n      let ss = date.getSeconds();\n      let session = \"AM\";\n\n      if (hh === 0) {\n        hh = 12;\n      }\n      if (hh > 12) {\n        hh = hh - 12;\n        session = \"PM\";\n      }\n\n      hh = hh < 10 ? \"0\" + hh : hh;\n      mm = mm < 10 ? \"0\" + mm : mm;\n      ss = ss < 10 ? \"0\" + ss : ss;\n\n      let time = hh + \":\" + mm + \":\" + ss + \" \" + session;\n\n      return time;\n    }\n    },\n    components: { App }\n}\n</script>"]},"metadata":{},"sourceType":"module"}