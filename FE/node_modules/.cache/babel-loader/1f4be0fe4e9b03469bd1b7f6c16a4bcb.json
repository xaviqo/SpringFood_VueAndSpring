{"ast":null,"code":"import App from '@/App.vue';\nexport default {\n  data: () => ({\n    drawer: true,\n    links: [{\n      icon: \"mdi-home\",\n      text: \"Home\",\n      route: \"/\"\n    }, {\n      icon: \"mdi-clipboard-text\",\n      text: \"Dashboard\",\n      route: \"/admin\"\n    }, {\n      icon: \"mdi-account-group\",\n      text: \"Team\",\n      route: \"/team\"\n    }, {\n      icon: \"mdi-food\",\n      text: \"Products\",\n      route: \"/products\"\n    }, {\n      icon: \"mdi-test-tube\",\n      text: \"Test\",\n      route: \"/generator\"\n    }]\n  }),\n  methods: {\n    currentTime() {\n      let date = new Date();\n      let hh = date.getHours();\n      let mm = date.getMinutes();\n      let ss = date.getSeconds();\n      let session = \"AM\";\n\n      if (hh === 0) {\n        hh = 12;\n      }\n\n      if (hh > 12) {\n        hh = hh - 12;\n        session = \"PM\";\n      }\n\n      hh = hh < 10 ? \"0\" + hh : hh;\n      mm = mm < 10 ? \"0\" + mm : mm;\n      ss = ss < 10 ? \"0\" + ss : ss;\n      let time = hh + \":\" + mm + \":\" + ss + \" \" + session;\n      return time;\n    }\n\n  },\n  components: {\n    App\n  }\n};","map":{"version":3,"mappings":"AAoFA;AACA;EACAA;IACAC,YADA;IAEAC,QACA;MACAC,gBADA;MAEAC,YAFA;MAGAC;IAHA,CADA,EAMA;MACAF,0BADA;MAEAC,iBAFA;MAGAC;IAHA,CANA,EAWA;MACAF,yBADA;MAEAC,YAFA;MAGAC;IAHA,CAXA,EAgBA;MACAF,gBADA;MAEAC,gBAFA;MAGAC;IAHA,CAhBA,EAqBA;MACAF,qBADA;MAEAC,YAFA;MAGAC;IAHA,CArBA;EAFA,EADA;EA+BAC;IACAC;MACA;MACA;MACA;MACA;MACA;;MAEA;QACAC;MACA;;MACA;QACAA;QACAC;MACA;;MAEAD;MACAE;MACAC;MAEA;MAEA;IACA;;EAvBA,CA/BA;EAwDAC;IAAAC;EAAA;AAxDA","names":["data","drawer","links","icon","text","route","methods","currentTime","hh","session","mm","ss","components","App"],"sourceRoot":"src/components","sources":["NavBar.vue"],"sourcesContent":["<template>\n    <nav>\n        <v-app-bar \n        app\n        elevation=\"0\"\n        dark\n        flat\n        short\n        color=\"green\">\n            <v-app-bar-nav-ic