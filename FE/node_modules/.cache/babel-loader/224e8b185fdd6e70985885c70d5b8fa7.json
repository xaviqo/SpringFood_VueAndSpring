   }\n      if (hh > 12) {\n          hh = hh - 12;\n          session = \"PM\";\n      }\n\n      hh = hh < 10 ? \"0\" + hh : hh;\n      mm = mm < 10 ? \"0\" + mm : mm;\n      ss = ss < 10 ? \"0\" + ss : ss;\n\n      return hh + \":\" + mm + \":\" + ss + \" \" + session;\n      }\n  },\n  components: { App },\n  mounted() {\n    this.pcTime = this.currentTime();\n    setInterval(() => {\n      this.pcTime = this.currentTime();\n    }, 300);\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}