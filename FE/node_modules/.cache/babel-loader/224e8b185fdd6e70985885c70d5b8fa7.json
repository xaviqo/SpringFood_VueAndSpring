{"ast":null,"code":"import App from \"@/App.vue\";\nexport default {\n  name: \"navbar\",\n  data: () => ({\n    drawer: false,\n    pcTime: \"\",\n    links: [{\n      icon: \"mdi-home\",\n      text: \"Home\",\n      route: \"/\",\n      show: true\n    }, {\n      icon: \"mdi-clipboard-text\",\n      text: \"Order Board\",\n      route: \"/admin\",\n      show: true\n    }, {\n      icon: \"mdi-account-group\",\n      text: \"Team Manager\",\n      route: \"/team\",\n      show: true\n    }, {\n      icon: \"mdi-food\",\n      text: \"Product Manager\",\n      route: \"/products\",\n      show: true\n    }, {\n      icon: \"mdi-test-tube\",\n      text: \"App Test\",\n      route: \"/generator\",\n      show: true\n    }]\n  }),\n  methods: {\n    currentTime() {\n      let date = new Date();\n      let hh = date.getHours();\n      let mm = date.getMinutes();\n      let ss = date.getSeconds();\n      let session = \"AM\";\n\n      if (hh === 0) {\n        hh = 12;\n      }\n\n      if (hh > 12) {\n        hh = hh - 12;\n        session = \"PM\";\n      }\n\n      hh = hh < 10 ? \"0\" + hh : hh;\n      mm = mm < 10 ? \"0\" + mm : mm;\n      ss = ss < 10 ? \"0\" + ss : ss;\n      return hh + \":\" + mm + \":\" + ss + \" \" + session;\n    }\n\n  },\n  components: {\n    App\n  },\n\n  mounted() {\n    this.pcTime = this.currentTime();\n    setInterval(() => {\n      this.pcTime = this.currentTime();\n    }, 300);\n  }\n\n};","map":{"version":3,"mappings":"AAiFA;AACA;EACAA,cADA;EAEAC;IACAC,aADA;IAEAC,UAFA;IAGAC,QACA;MACAC,gBADA;MAEAC,YAFA;MAGAC,UAHA;MAIAC;IAJA,CADA,EAOA;MACAH,0BADA;MAEAC,mBAFA;MAGAC,eAHA;MAIAC;IAJA,CAPA,EAaA;MACAH,yBADA;MAEAC,oBAFA;MAGAC,cAHA;MAIAC;IAJA,CAbA,EAmBA;MACAH,gBADA;MAEAC,uBAFA;MAGAC,kBAHA;MAIAC;IAJA,CAnBA,EA0BA;MACAH,qBADA;MAEAC,gBAFA;MAGAC,mBAHA;MAIAC;IAJA,CA1BA;EAHA,EAFA;EAuCAC;IACAC;MACA;MACA;MACA;MACA;MACA;;MAEA;QACAC;MACA;;MACA;QACAA;QACAC;MACA;;MAEAD;MACAE;MACAC;MAEA;IACA;;EArBA,CAvCA;EA8DAC;IAAAC;EAAA,CA9DA;;EA+DAC;IACA;IACAC;MACA;